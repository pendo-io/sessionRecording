{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "allowJs": true,
    "outDir": "lib",
    "target": "esnext",
    "module": "esnext",
    "lib": ["DOM","esnext"],
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
  },
  "include": ["js/*"],
}